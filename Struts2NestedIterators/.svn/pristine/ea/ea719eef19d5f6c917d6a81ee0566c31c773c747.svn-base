 * and open the template in the editor.
 */



public class ListBean
{
    private ArrayList<String> lstString=new ArrayList<String>();
    private String name;
    private Double amt;
    private Integer id;
    public ListBean()
    {

    }
    public ListBean(int i, double d, String string, ArrayList<String> lstString) {
        id=i;
        amt=d;
        name=string;
        this.lstString=lstString;
    }

    public Double getAmt() {
        return amt;
    }

    public void setAmt(Double amt) {
        this.amt = amt;
    }

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public List<String> getLstString() {
        return lstString;
    }

    public void setLstString(ArrayList<String> lstString) {
        this.lstString = lstString;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

}
