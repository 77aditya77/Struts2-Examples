<struts>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default">

        <!-- This is a pass-through action which doesn't require a class -->
        <action name="pass">
            <result>/home.jsp</result>
        </action>
    </package>
</struts>
